{
  "cSpell.words": ["Vitesse"],
  "prettier.enable": false,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true,
  },
  "files.associations": {
    "*.css": "postcss",
  },
  "i18n-ally.localesPaths": [
    "locales"
  ],
  "editor.quickSuggestions": {
    "strings": true
  },
  "css.validate": false,
  "i18n-ally.keystyle": "nested"
}
